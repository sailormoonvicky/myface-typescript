<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1><%= name %></h1>
<img src= <%= coverImageUrl %> alt="An image a user cover">
<p><%= username %> <%= email %></p>
<img src= <%= profileImageUrl %> alt="An image a user profile">
<ol>
    <% posts.forEach((post) => {%>
        <li>
            <p>Post: <%= post.message %></p>
            <img src="<%= post.imageUrl %>" alt="An image of post">
            <p>Created time: <%= post.createdAt %></p>
        </li>
<% }) %>
</ol>
</body>
<%- include('_footer') -%>
</html>